FUNCTION mult_bao (dataa[6..0], datab[14..0])
RETURNS ( result[21..0]);

--synthesis_resources = lut 139 
SUBDESIGN mac_mult_h3h1
( 
	dataa[6..0]	:	input;
	datab[14..0]	:	input;
	dataout[21..0]	:	output;
	signa	:	input;
	signb	:	input;
) 
VARIABLE 
	mult1 : mult_bao;
	const_node_signa	: NODE;
	const_node_signb	: NODE;
	tmp_dataout[21..0]	: WIRE;
	x_dataa[6..0]	: WIRE;
	x_datab[14..0]	: WIRE;
	x_output[21..0]	: WIRE;

BEGIN 
	mult1.dataa[] = x_dataa[];
	mult1.datab[] = x_datab[];
	dataout[] = tmp_dataout[];
	const_node_signa = signa;
	const_node_signb = signb;
	tmp_dataout[] = x_output[];
	x_dataa[] = ( dataa[]);
	x_datab[] = ( datab[]);
	x_output[21..0] = mult1.result[21..0];
END;
